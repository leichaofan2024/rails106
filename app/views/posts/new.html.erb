<h2>写文章</h2>
<div class="col-md-4 col-md-offset-4">
  <%= simple_form_for [@group,@post] do |f| %>
  <%= f.input :title, input_html: { class: "control-form"} %>
  <%= f.input :content, input_html: {class: "control-form"} %>
  <%= f.submit "Submit", disable_with: "Submiting..." %>
  <% end %>
</div>
